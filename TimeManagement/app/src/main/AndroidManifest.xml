<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tr.timemanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.tr.timemanagement.TimeManagementApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tr.timemanagement.ui.auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tr.timemanagement.ui.UserSettingsActivity" />
        <activity android:name="com.tr.timemanagement.ui.UserListActivity" />
        <activity android:name="com.tr.timemanagement.ui.UserTimeActivity" />
        <activity android:name="com.tr.timemanagement.ui.auth.RegisterActivity" />
        <activity android:name="com.tr.timemanagement.ui.ManagerListActivity" />
        <activity android:name="com.tr.timemanagement.ui.SelectListActivity" />
        <activity android:name="com.tr.timemanagement.ui.AdminListActivity" />
        <activity android:name="com.tr.timemanagement.ui.ManagerSettingsActivity" />
        <activity android:name="com.tr.timemanagement.ui.AdminSettingsActivity"></activity>
    </application>

</manifest>